{
  "name": "iszsw/mock",
  "description": "根据注解自动生成 生成路由 + 生成接口文档 + Mock测试数据",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "zsw",
      "email": "zswemail@qq.com"
    }
  ],
  "keywords": [
    "doc",
    "mock",
    "annotation"
  ],
  "require": {
    "doctrine/annotations": "^1.6",
    "php-di/phpdoc-reader": "^2.1",
    "symfony/class-loader": "~3.2.0",
    "fzaninotto/faker": "^1.9"
  },
  "extra": {
    "think":{
      "services":[
        "iszsw\\mock\\Service"
      ],
      "config":{
        "mock": "src/config.php"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "iszsw\\mock\\": "src"
    }
  }
}
